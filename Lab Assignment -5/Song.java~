public class Song{
  public String name;
  public String singer;
  public int duration;
  public Song next;
 public Song(String a,String b,int c){
  name=a;
  singer=b;
  duration=c;
  next=null;
 }
 public void songInfo(){
   System.out.println("Title: "+name+",  Artist: "+singer+" \nLength: "+duration+" minutes");
 }
}