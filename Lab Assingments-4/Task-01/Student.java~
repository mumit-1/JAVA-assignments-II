public class Student {
    public String name;
    private String program;
    public Student(String a,String b) {
        name = a;
        program = b;
    }
    public void updateName(String newNama) {
        name = a;
    }
    public void updateProgram(String newPrograa) {
        program = a;
    }
    public String accessProgram() {
        return program;
    }
}