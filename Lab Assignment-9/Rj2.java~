public class Rj2 {
    public static void main (String [] args) {
        
        int arr[] = {9, -5, 7, -5, 5, 7};
        int len = arr.length;
        int count = 0;
      
         for ( int i = 0; i<(len/2); i++ ) {
            len--;
         arr[i]=arr[len]
        }
          for ( int i = 0; i<len; i++ ) {
         System.out.print(arr[i]+",");
        }
    }
}